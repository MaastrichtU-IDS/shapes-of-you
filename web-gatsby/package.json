    "scripts": {
        "build": "gatsby build",
        "dev": "gatsby develop",
        "serve": "gatsby build && gatsby serve -p 8000",
        "clean": "gatsby clean",
        "publish": "gatsby build --prefix-paths && gh-pages -d public",
        "style": "yarn run format && yarn run lint",
        "format": "yarn prettier --ignore-path .eslintignore ./src --write --config ./.prettierrc",
        "check": "yarn prettier --ignore-path .eslintignore ./src --check --config ./.prettierrc",
        "lint": "yarn eslint ./src --cache --config ./.eslintrc.json",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
